{
  "agent": {
    "name": "classify-agent-gpt4o-entra",
    "transport": {
      "provider": {
        "name": "azure",
        "base_url": "https://go-agents-platform.openai.azure.com/openai",
        "model": {
          "name": "gpt-4o",
          "capabilities": {
            "vision": {
              "format": "vision",
              "options": {
                "max_tokens": 4096,
                "temperature": 0.1,
                "detail": "high"
              }
            }
          }
        },
        "options": {
          "deployment": "gpt-4o",
          "api_version": "2025-01-01-preview",
          "auth_type": "bearer"
        }
      }
    }
  }
}
