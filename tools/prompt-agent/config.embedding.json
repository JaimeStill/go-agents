{
  "name": "embeddings-agent",
  "transport": {
    "provider": {
      "name": "ollama",
      "base_url": "http://localhost:11434",
      "model": {
        "name": "embeddinggemma:300m",
        "capabilities": {
          "embeddings": {
            "format": "embeddings",
            "options": {
              "dimensions": 768
            }
          }
        }
      }
    },
    "timeout": "24s",
    "max_retries": 3,
    "retry_backoff_base": "1s",
    "connection_pool_size": 10,
    "connection_timeout": "6s"
  }
}
