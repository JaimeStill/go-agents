{
  "name": "vision-agent",
  "transport": {
    "provider": {
      "name": "ollama",
      "base_url": "http://localhost:11434",
      "model": {
        "name": "gemma3:4b",
        "format": "openai-standard",
        "options": {
          "max_tokens": 4096,
          "temperature": 0.7
